        />\n            </div>\n            <button onClick={onSubmit} className=\"btn btn-success btn-block\">\n              Submit\n            </button>\n          </form>\n        </div>\n      </div>\n    );\n  };\n\n  return (\n    <Base title=\"Sign In page\" description=\"A page for user to sign in!\">\n      {loadingMessage()}\n      {errorMessage()}\n      {signInForm()}\n      {performRedirect()}\n      <p className=\"text-white text-center\">{JSON.stringify(values)}</p>\n    </Base>\n  );\n};\n\nexport default Signin;\n","E:\\MERN\\portfolio-e-commerce-store\\project-frontend\\src\\auth\\helper\\index.js",[],"E:\\MERN\\portfolio-e-commerce-store\\project-frontend\\src\\auth\\helper\\PrivateRoutes.js",[],"E:\\MERN\\portfolio-e-commerce-store\\project-frontend\\src\\auth\\helper\\AdminRoutes.js",[],"E:\\MERN\\portfolio-e-commerce-store\\project-frontend\\src\\user\\UserDashBoard.js",[],"E:\\MERN\\portfolio-e-commerce-store\\project-frontend\\src\\user\\AdminDashBoard.js",["70"],{"ruleId":"71","severity":1,"message":"72","line":4,"column":8,"nodeType":"73","messageId":"74","endLine":4,"endColumn":11},{"ruleId":"71","severity":1,"message":"75","line":6,"column":10,"nodeType":"73","messageId":"74","endLine":6,"endColumn":23},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"71","severity":1,"message":"80","line":7,"column":8,"nodeType":"73","messageId":"74","endLine":7,"endColumn":18},{"ruleId":"71","severity":1,"message":"81","line":5,"column":8,"nodeType":"73","messageId":"74","endLine":5,"endColumn":12},{"ruleId":"71","severity":1,"message":"82","line":3,"column":10,"nodeType":"73","messageId":"74","endLine":3,"endColumn":14},{"ruleId":"71","severity":1,"message":"83","line":7,"column":34,"nodeType":"73","messageId":"74","endLine":7,"endColumn":38},"no-unused-vars","'App' is defined but never used.","Identifier","unusedVar","'BrowserRouter' is defined but never used.","no-native-reassign",["84"],"no-negated-in-lhs",["85"],"'Ad